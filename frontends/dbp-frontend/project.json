{
  "name": "frontends-dbp-frontend",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "frontends/dbp-frontend/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/frontends/dbp-frontend",
        "index": "frontends/dbp-frontend/src/index.html",
        "baseHref": "/",
        "main": "frontends/dbp-frontend/src/main.tsx",
        "tsConfig": "frontends/dbp-frontend/tsconfig.app.json",
        "assets": [
          "frontends/dbp-frontend/src/favicon.ico",
          "frontends/dbp-frontend/src/assets"
        ],
        "styles": ["frontends/dbp-frontend/src/styles.css"],
        "scripts": [],
        "webpackConfig": "frontends/dbp-frontend/webpack.config.js",
        "postcssConfig": "frontends/dbp-frontend/postcss.config.js"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "frontends/dbp-frontend/src/environments/environment.ts",
              "with": "frontends/dbp-frontend/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "serve": {
      "executor": "@nx/webpack:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "frontends-dbp-frontend:build",
        "hmr": true
      },
      "configurations": {
        "development": {
          "buildTarget": "frontends-dbp-frontend:build:development"
        },
        "production": {
          "buildTarget": "frontends-dbp-frontend:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "frontends/dbp-frontend/jest.config.ts"
      }
    }
  },
  "tags": []
}
